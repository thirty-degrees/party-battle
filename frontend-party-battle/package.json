{
  "name": "frontend-party-battle",
  "private": true,
  "main": "expo-router/entry",
  "version": "2.16.0",
  "type": "commonjs",
  "scripts": {
    "start": "npm run -w types-party-battle build && npx expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web -c",
    "check-types": "tsc --noEmit",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "lint:ci": "expo lint -- --format junit --output-file lint-results.xml",
    "test": "jest",
    "test:ci": "JEST_JUNIT_OUTPUT_NAME=test-results.xml jest --ci --testResultsProcessor=jest-junit",
    "docker:build": "docker build -f ../frontend-party-battle/dockerfile -t frontend-party-battle ..",
    "build": "npm run -w types-party-battle build",
    "prebuild": "npm run -w types-party-battle build",
    "build:ios": "eas build --platform ios --profile development --local",
    "kill-port": "lsof -ti:8081 | xargs kill -9",
    "scan": "npx react-scan http://localhost:8081/"
  },
  "dependencies": {
    "@expo-google-fonts/lilita-one": "^0.4.1",
    "@expo/html-elements": "^0.10.1",
    "@expo/vector-icons": "^15.0.2",
    "@gluestack-ui/core": "^3.0.10",
    "@gluestack-ui/utils": "^3.0.7",
    "@legendapp/motion": "^2.4.0",
    "@rive-app/react-canvas": "4.23.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "colyseus.js": "^0.16.19",
    "expo": "~54.0.11",
    "expo-blur": "^15.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-constants": "~18.0.9",
    "expo-device": "~8.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.9",
    "expo-splash-screen": "~31.0.10",
    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "isomorphic-ws": "^5.0.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "react-stately": "^3.41.0",
    "rive-react-native": "^9.6.1",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.4.17",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-formatter-junit": "^8.40.0",
    "eslint-plugin-prettier": "^5.5.4",
    "expo-dev-client": "~6.0.13",
    "jest-expo": "~54.0.12",
    "jest-junit": "^16.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  }
}
