{
  "name": "frontend-party-battle",
  "private": true,
  "main": "expo-router/entry",
  "version": "2.10.0",
  "type": "commonjs",
  "scripts": {
    "start": "npm run -w types-party-battle build && npx expo start -c",
    "android": "expo start --android -c",
    "ios": "expo start --ios -c",
    "web": "expo start --web -c",
    "check-types": "tsc --noEmit",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "lint:ci": "expo lint -- --format junit --output-file lint-results.xml",
    "test": "jest",
    "test:ci": "JEST_JUNIT_OUTPUT_NAME=test-results.xml jest --ci --testResultsProcessor=jest-junit",
    "docker:build": "docker build -f ../frontend-party-battle/dockerfile -t frontend-party-battle ..",
    "build": "npm run -w types-party-battle build",
    "prebuild": "npm run -w types-party-battle build"
  },
  "dependencies": {
    "@expo/html-elements": "^0.10.1",
    "@expo/vector-icons": "^15.0.2",
    "@gluestack-ui/core": "^3.0.10",
    "@gluestack-ui/utils": "^3.0.7",
    "@legendapp/motion": "^2.4.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "colyseus.js": "^0.16.19",
    "expo": "~54.0.8",
    "expo-blur": "^15.0.7",
    "expo-build-properties": "^1.0.8",
    "expo-constants": "~18.0.9",
    "expo-device": "^8.0.7",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.6",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "isomorphic-ws": "^5.0.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-aria": "^3.43.2",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-animatable": "^1.4.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.13.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "react-stately": "^3.41.0",
    "stream-browserify": "^3.0.0",
    "tailwind-variants": "^0.1.20",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "expo-dev-client": "^6.0.12",
    "typescript": "~5.9.2"
  }
}
